Function AcceptDelivery(dReceived As Date) As Boolean
   Dim loCPTrans As clsCPInventoryTrans
   Dim lors As Recordset
   Dim lsProcName As String
   Dim lsSQL As String
   Dim lnRow As Long
   
   lsProcName = "AcceptDelivery"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   AcceptDelivery = False
   
   If Not pbInitTran Then GoTo endProc
   
   With p_oBaseClas
      If .Master("sTransNox") = "" Then GoTo endProc
      
      lsSQL = "SELECT" & _
                  "  a.sStockIDx" & _
                  ", a.nQuantity" & _
                  ", b.nQtyOnHnd" & _
                  ", b.nLedgerNo" & _
               " FROM " & .DetailTable & " a" & _
                     " LEFT JOIN CP_Inventory_Master b" & _
                        " ON a.sStockIDx = b.sStockIDx" & _
                           " AND b.sBranchCd = " & strParm(.Master("sDestinat")) & _
               " WHERE a.sTransNox = " & strParm(.Master("sTransNox")) & _
               " ORDER BY a.nEntryNox"
      
      Set lors = New Recordset
      lors.Open lsSQL, p_oAppDrivr.Connection, , , adCmdText
      
      If lors.RecordCount <> .ItemCount Then
         MsgBox "Data Discrepancy Detected!", vbCritical, "Warning"
         GoTo endProc
      End If
      
      Set loCPTrans = New clsCPInventoryTrans
      With loCPTrans
         Set .AppDriver = p_oAppDrivr
         .Branch = p_oBaseClas.Master("sDestinat")
         If .InitTransaction() = False Then GoTo endProc
      End With
      
      Set loCPTrans = New clsCPInventoryTrans
      With loCPTrans
         .Branch = p_sBranchCd
         Set .AppDriver = p_oAppDrivr
         If .InitTransaction() = False Then GoTo endProc
      End With
      
      'Leave this part first...
      For pnCtr = 0 To .ItemCount - 1
         loCPTrans.Detail(pnCtr, "sStockIDx") = lors("sStockIDx")
         loCPTrans.Detail(pnCtr, "nQuantity") = lors("nQuantity")
         If IsNull(lors("nQtyOnHnd")) Then
            loCPTrans.Detail(pnCtr, "cNewParts") = xeYes
            loCPTrans.Detail(pnCtr, "nQtyOnHnd") = 0
         Else
            loCPTrans.Detail(pnCtr, "nQtyOnHnd") = lors("nQtyOnHnd")
            loCPTrans.Detail(pnCtr, "nLedgerNo") = lors("nLedgerNo")
         End If
         
         lors.MoveNext
      Next
      
      ' if a CP_Inventory is not successfully updated, cancel saving.
      If loCPTrans.AcceptDelivery(.Master("sTransNox"), _
            dReceived, _
            xeModeAddNew) = False Then
         GoTo endProc
      End If
      
      lsSQL = "UPDATE " & .MasterTable & " SET" & _
                  "  sReceived = " & strParm(p_oAppDrivr.UserID) & _
                  ", dReceived = " & dateParm(dReceived) & _
                  ", cTranStat = " & strParm(xeStatePosted) & _
                  ", dModified = " & dateParm(p_oAppDrivr.ServerDate()) & _
               " WHERE sTransNox = " & strParm(.Master("sTransNox"))
               
      lnRow = p_oAppDrivr.Execute(lsSQL, _
                  .MasterTable, _
                  p_sBranchCd, _
                  Left(.Master("sTransNox"), 2))
      
      If lnRow = 0 Then
         MsgBox "Unable to Update Delivery Transaction!!!", vbCritical, "Warning"
         GoTo endProc
      End If
      
      ' after accepting parts, check for SP Requested Disapproval
      'if save
   End With
   
   AcceptDelivery = True
   
endProc:
   Set loCPTrans = Nothing

   Exit Function
errProc:
   ShowError lsProcName & "( " & dReceived & " )"
End Function

Private Sub getMaster(ByVal lsValue As String, ByVal lbSearch As Boolean)
   Dim lsMaster As String
   Dim lasMaster() As String
   Dim lsProcName As String

   lsProcName = "GetMaster"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   
   With p_oBaseClas
      If lsValue <> "" Then
         If lsValue = .Master(2) Then GoTo endProc
         
         If lbSearch Then
            lsMaster = "a.sBranchNm LIKE " & strParm(Trim(lsValue) & "%")
         Else
            lsMaster = "a.sBranchNm = " & strParm(Trim(lsValue))
         End If
      ElseIf lbSearch = False Then
         GoTo endWithClear
      End If
   
      lsMaster = .getMaster(2, lsMaster)
      If lsMaster = Empty Then
         If lbSearch = False Then
            GoTo endWithClear
         Else
            GoTo endProc
         End If
      End If
      lasMaster = Split(lsMaster, "»")
      
      .Master(2) = lasMaster(1)
      p_sAddressx = lasMaster(2)
   End With

endProc:
   RaiseEvent MasterRetrieved(2)
   RaiseEvent MasterRetrieved(3)

   Exit Sub
endWithClear:
   With p_oBaseClas
      .Master(2) = ""
      p_sAddressx = ""
   End With
   GoTo endProc
errProc:
    ShowError lsProcName & "( " & lsValue _
                        & ", " & lbSearch & " )"
End Sub
                     
Private Sub getDetail(ByVal lnRow As Integer, _
                        ByVal lnIndex As Integer, _
                        ByVal lsValue As String, _
                        ByVal lbSearch As Boolean)
   Dim lsDetail As String
   Dim lasDetail() As String
   Dim lsProcName As String

   lsProcName = "GetDetail"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   
   With p_oBaseClas
      If lsValue <> "" Then
         If lnIndex = 1 Then     ' Bar Code
            If lsValue = .Detail(lnRow, "sBarrCode") Then GoTo endProc
            If lbSearch Then
               lsDetail = "a.sBarrCode LIKE " & strParm(Trim(lsValue) & "%")
            Else
               lsDetail = "a.sBarrcode = " & strParm(Trim(lsValue))
            End If
         Else                    ' Description
            If lsValue = .Detail(lnRow, "sDescript") Then GoTo endProc
            If lbSearch Then
               lsDetail = "a.sDescript LIKE " & strParm(Trim(lsValue) & "%")
            Else
               lsDetail = "a.sDescript = " & strParm(Trim(lsValue))
            End If
         End If
      ElseIf lbSearch = False Then
         GoTo endWithClear
      End If
   
      lsDetail = .getDetail(lnRow, lsDetail)
      If lsDetail = Empty Then
         If lbSearch = False Then
            GoTo endWithClear
         Else
            GoTo endProc
         End If
      End If
      lasDetail = Split(lsDetail, "»")

      .Detail(lnRow, "sBarrCode") = lasDetail(0)
      .Detail(lnRow, "sDescript") = lasDetail(1)
      .Detail(lnRow, "sModelNme") = lasDetail(2)
      .Detail(lnRow, "nQtyOnHnd") = CLng(lasDetail(3))
      .Detail(lnRow, "nQuantity") = 0
      .Detail(lnRow, "nUnitPrce") = CDbl(lasDetail(4))
      .Detail(lnRow, "sStockIDx") = lasDetail(5)
      .Detail(lnRow, "nLedgerNo") = lasDetail(6)
      .Detail(lnRow, "nLedgerNo") = lasDetail(6)
      .Detail(lnRow, "cHsSerial") = lasDetail(7)
   
   End With

endProc:
   RaiseEvent DetailRetrieved(1)
   RaiseEvent DetailRetrieved(2)
   RaiseEvent DetailRetrieved(3)
   RaiseEvent DetailRetrieved(4)
   RaiseEvent DetailRetrieved(5)
   RaiseEvent DetailRetrieved(6)
   RaiseEvent DetailRetrieved(7)

   Exit Sub
endWithClear:
   With p_oBaseClas
      .Detail(lnRow, "sBarrCode") = ""
      .Detail(lnRow, "sDescript") = ""
      .Detail(lnRow, "sModelNme") = ""
      .Detail(lnRow, "nQtyOnHnd") = 0
      .Detail(lnRow, "nUnitPrce") = 0#
      .Detail(lnRow, "nQuantity") = 0
      .Detail(lnRow, "sStockIDx") = ""
      .Detail(lnRow, "nLedgerNo") = 0
      .Detail(lnRow, "cHsSerial") = xeNo
      
   End With
   GoTo endProc
errProc:
    ShowError lsProcName & "( " & lnRow _
                        & ", " & lnIndex _
                        & ", " & lsValue _
                        & ", " & lbSearch & " )"
End Sub

Private Function delSPTransaction() As Boolean
   Dim loCPTrans As clsCPInventoryTrans
   Dim lsProcName As String
   Dim lbHsSerial As Boolean
   
   lsProcName = "delSPTransaction"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   
   Set loCPTrans = New clsCPInventoryTrans
   With loCPTrans
      Set .AppDriver = p_oAppDrivr
      .Branch = p_sBranchCd
      If .InitTransaction() = False Then GoTo endProc
      
      For pnCtr = 0 To .ItemCount - 1
         .Detail(pnCtr, "sStockIDx") = p_oBaseClas.Detail(pnCtr, "sStockIDx")
         .Detail(pnCtr, "nQtyOnHnd") = p_oBaseClas.Detail(pnCtr, "nQtyOnHnd")
         .Detail(pnCtr, "nLedgerNo") = p_oBaseClas.Detail(pnCtr, "nLedgerNo")
      
         'Delete serial of this stock if serialized
         If lbHasSerial = False And .Detail(pnCtr, "cHsSerial") = xeYes Then
            lbHasSerial = True
         End If
            
      Next
      
      If lbHsSerial = True Then
         If deleteSerial() = False Then
            MsgBox "Unable to delete serials of this STOCK: " & .Detail(pnCtr, "sStockIDx"), vbCritical, "Warning"
            GoTo endProc
         End If
      End If
      
      ' if a serial is not successfully updated, cancel saving.
      If .Delivery(p_oBaseClas.Master("sTransNox"), _
            p_oBaseClas.Master("dTransact"), _
            xeModeDelete) = False Then
         GoTo endProc
      End If
   End With
   
   delSPTransaction = True
   
endProc:
   Exit Function
errProc:
   ShowError lsProcName & "( " & " )"
End Function

Private Function createSerialTable() As Boolean
   Dim lsProcName As String
   
   lsProcName = "createSerialTable"
   On Error GoTo errProc
   
   Set p_oSerial = New Recordset
   With p_oSerial
      .Fields.Append "nEntryNox", adInteger, 4
      .Fields.Append "sSerialID", adVarChar, 10
      .Fields.Append "sSerialNo", adVarChar, 20
      .Fields.Append "sTransNox", adVarChar, 10
      .Open
   End With

   createSerialTable = True
   
endProc:
   Exit Function
errProc:
   ShowError lsProcName & "( " & " )"
End Function

Private Sub addSerial(ByVal EntryNo As Variant, ByVal Quantity As Variant)
   Dim lsProcName As String
   Dim lnRow As Integer
   
   lsProcName = "addSerial"
   On Error GoTo errProc
   
   With p_oSerial
      For lnRow = 0 To Quantity - 1
         .AddNew
         .Fields("nEntryNox") = EntryNo
         .Fields("sSerialNo") = ""
         .Fields("sSerialID") = ""
         .Fields("sTransNox") = p_oBaseClas.Master("sTransNox")
      Next
   End With

endProc:
   Exit Sub
errProc:
   ShowError lsProcName & "( " & " )"
End Sub

Private Function addSerialOnCache( _
      lnEntryNox As Integer, _
      lsSerialID As String, _
      lnOldEntry As Integer _
      ) As Boolean
   Dim lsProcName As String
   
   lsProcName = "addSerialOnCache"
   On Error GoTo errProc

   With p_oSerialOld
      .Filter = ""
      If .RecordCount = 0 Then
         .AddNew
         .Fields("sTransNox") = p_oBaseClas.Master("sTransNox")
      Else
         .MoveFirst
         .Find "nEntryNox = " & strParm(lnOldEntry) & _
          " AND sSerialID = " & strParm(lsSerialID), 0, adSearchForward
         
         If .EOF Then
            .AddNew
            .Fields("sTransNox") = p_oBaseClas.Master("sTransNox")
         End If
      End If
      
      .Fields("nEntryNox") = lnEntryNox + 1
      .Fields("sSerialID") = lsSerialID
   
   End With
   
   addSerialOnCache = True
   
endProc:
   Exit Function
errProc:
   ShowError lsProcName & "( " & " )"
End Function

Private Function loadSerialOnCache() As Boolean
   Dim lsSQL As String
   Dim lors As adodb.Recordset
   Dim lsProcName As String
   
   lsProcName = "loadSerialOnCache"
   On Error GoTo errProc

   lsSQL = "SELECT a.sSerialID" & _
                ", b.sSerialNo" & _
                ", a.nEntryNox" & _
                ", a.sTransNox" & _
          " FROM CP_Transfer_Serial a" & _
              ", CP_Inventory_Serial b" & _
          " WHERE a.sTransNox = " & strParm(.Fields("sTransNox")) & _
            " AND a.sSerialID = b.sSerialID" & _
          " ORDER BY a.nEntryNox"
  
   Set p_oSerialOld = New adodb.Recordset
   p_oSerialOld.Open lsSQL, p_oAppDrivr, adOpenKeyset, adLockOptimistic, adCmdText
   Set p_oSerialOld.ActiveConnection = Nothing
      
   createSerialTable

   Do Until lors.EOF
      p_oSerial.AddNew
      p_oSerial("nEntryNox") = p_oSerialOld("nEntryNox")
      p_oSerial("sSerialID") = p_oSerialOld("sSerialID")
      p_oSerial("sSerialNo") = p_oSerialOld("sSerialNo")
      p_oSerial("sTransNox") = p_oBaseClas.Master("sTransNox")
                  
      p_oSerialOld.MoveNext
   Loop
   
   loadSerialOnCache = True
   
endProc:
   Exit Function
errProc:
   ShowError lsProcName & "( " & " )"
End Function

Private Function delSerialOnCache(ByVal lnEntryNox As Integer) As Boolean
   Dim lsSQL As String
   Dim lors As adodb.Recordset
   Dim lsProcName As String
   
   lsProcName = "delSerialOnCache"
   On Error GoTo errProc
   
   With p_oSerial
      .Filter = "nEntryNox = " & lnEntryNox

      If Not .EOF Then .MoveFirst
      Do Until .EOF
         .Delete
         .MoveNext
      Loop
   End With
      
   delSerialOnCache = True
   
endProc:
   Exit Function
errProc:
   ShowError lsProcName & "( " & " )"
End Function

Private Function saveSerial( _
      ByVal lnEntryNox As Integer, _
      ByVal lnQuantity As Integer, _
      ByVal lnOldEntry As Integer _
      ) As Boolean
   
   Dim lors As adodb.Recordset
   Dim lsProcName As String
   Dim lsSerialID As String
   Dim lnSerial As Integer
   Dim lsSerial As String
   Dim lnCtr As Integer
   Dim lsSQL As String
   
   lsProcName = "saveSerial"
'   On Error GoTo errProc

   With p_oSerial
      .Filter = "nEntryNox = " & lnOldEntry
      If .RecordCount < lnQuantity Then
         MsgBox "No of items in serial is not equal with the quantity specified!"
         GoTo endProc
      Else
         'Assumption was that we can delete a record but not insert in between records
         If p_oBaseClas.EditMode <> xeModeAddNew And lnEntryNox <> lnOldEntry Then
            p_oSerialOld.Filter = "nEntryNox = " & lnEntryNox
            If p_oSerialOld.RecordCount > 0 Then
               lsSQL = "DELETE FROM CP_Transfer_Serial" & _
                      " WHERE sTransNox = " & strParm(.Fields("sTransNox")) & _
                        " AND nEntryNox = " & lnEntryNox
               p_oAppDrivr.Execute lsSQL, "CP_Transfer_Serial", Left(.Fields("sTransNox"), 2)
            End If
         End If
         
         .MoveFirst
         lsSerial = ""
         lsSerialID = GetNextCode("CP_Inventory_Serial", "sSerialID", True, p_oAppDrivr.Connection, True, p_sBranchCd)
         lnSerial = CLng(Mid(lsSerialID, 5))
         
         For lnCtr = 1 To lnQuantity
            .Fields("sSerialID") = Left(lsSerialID, 4) & Format(lnSerial, String(6, "0"))
            If addSerialOnCache( _
               lnEntryNox, _
               .Fields("sSerialID"), _
               lnOldEntry) = False Then
               
               MsgBox "Unable to add serial!"
               GoTo endProc
            End If
            
            If IFNull(p_oSerialOld.Fields("sTransNox").OriginalValue) = "" Then
               .Fields("sTransNox") = p_oBaseClas.Master("sTransNox")
               lsSQL = ""
            Else
               lsSQL = "sTransNox = " & strParm(.Fields("sTransNox")) & _
                  " AND nEntryNox = " & lnOldEntry & _
                  " AND sSerialID = " & strParm(.Fields("sSerialID"))
            End If
            
            .Fields("nEntryNox") = lnEntryNox
                        
            lsSQL = ADO2SQL( _
                        p_oSerialOld, _
                        "CP_Transfer_Serial", _
                        lsSQL, , , _
                        "sSerialNo")
            
            If lsSQL <> "" Then
               p_oAppDrivr.Execute lsSQL, "CP_Transfer_Serial", Left(.Fields("sTransNox"), 2)
            End If
                        
            'Get all serials
            lsSerial = lsSerial & ", " & .Fields("sSerialID")
            
            p_oCPTrans.Serial(lnEntryNox, lnCtr - 1, "sSerialID") = .Fields("sSerialID")
            p_oCPTrans.Serial(lnEntryNox, lnCtr - 1, "sSerialNo") = .Fields("sSerialNo")
            lnSerial = lnSerial + 1
            .MoveNext
         Next
         
         'Delete previous serials that are not existing up to this time
         If p_oBaseClas.EditMode <> xeModeAddNew Then
            'Delete based on Old nEntryNox
            lsSQL = "SELECT * " & _
                   " FROM CP_Transfer_Serial" & _
                   " WHERE sTransNox = " & strParm(.Fields("sTransNox")) & _
                     " AND nEntryNox = " & lnOldEntry & _
                     " AND sSerialID NOT IN (" & Mid(lsSerial, 2) & ")"
            Set lors = p_oAppDrivr.Connection.Execute(lsSQL, , adCmdText)
            
            If lors.RecordCount > 0 Then
               lsSQL = "DELETE FROM CP_Transfer_Serial" & _
                      " WHERE sTransNox = " & strParm(.Fields("sTransNox")) & _
                        " AND nEntryNox = " & lnOldEntry & _
                        " AND sSerialID NOT IN (" & Mid(lsSerial, 2) & ")"
               p_oAppDrivr.Execute lsSQL, "CP_Transfer_Serial", Left(.Fields("sTransNox"), 2)
            End If
         End If
      End If
   End With
   
   saveSerial = True
   
endProc:
   Exit Function
errProc:
   ShowError lsProcName & "( " & " )"
End Function

Private Function deleteSerial() As Boolean
   Dim lsProcName As String
   Dim lsSQL As String
   Dim lors As adodb.Recordset
   Dim lnCtr As Integer
   
   lsProcName = "deleteSerial"
   On Error GoTo errProc

   With p_oSerial
      lsSQL = "DELETE FROM CP_Transfer_Serial" & _
             " WHERE sTransNox = " & strParm(.Fields("sTransNox"))
      p_oAppDrivr.Execute lsSQL, "CP_Transfer_Serial", Left(.Fields("sTransNox"), 2), .Fields("sDestinat")
   End With
   
   deleteSerial = True
   
endProc:
   Exit Function
errProc:
   ShowError lsProcName & "( " & " )"
End Function

