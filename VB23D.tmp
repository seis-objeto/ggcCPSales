Private Sub p_oBaseClas_InitMaster()
   Dim lsProcName As String
   
   lsProcName = "p_oBaseClas_InitMaster"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   
   With p_oBaseClas
      .Master("sTransNox") = GetNextCode(.MasterTable, "sTransNox", True, _
                                 p_oAppDrivr.Connection, True, p_sBranchCd)
      .Master("dTransact") = p_oAppDrivr.ServerDate
      .Master("sDestinat") = ""
      .Master("sReferNox") = ""
      .Master("sRequestx") = ""
      .Master("sRemarksx") = ""
      .Master("sReceived") = ""
      .Master("dReceived") = p_oAppDrivr.ServerDate
      .Master("sApproved") = ""
      .Master("cTranStat") = xeStateOpen
      .Master("nEntryNox") = 1
      
      .Detail(0, "nEntryNox") = 1
      .Detail(0, "sBarrCode") = ""
      .Detail(0, "sDescript") = ""
      .Detail(0, "sModelNme") = ""
      .Detail(0, "nQtyOnHnd") = 0
      .Detail(0, "nUnitPrce") = 0#
      .Detail(0, "nQuantity") = 0
      .Detail(0, "sTransNox") = .Master("sTransNox")
      .Detail(0, "sStockIDx") = ""
      .Detail(0, "nLedgerNo") = 0
      .Detail(0, "cHsSerial") = xeNo
   End With

   Call createSerialTable
   
   'Initialized Old serial
   lsSQL = "SELECT a.sSerialID" & _
                ", b.sSerialNo" & _
                ", a.nEntryNox" & _
                ", a.sTransNox" & _
          " FROM CP_Transfer_Serial a" & _
              ", CP_Inventory_Serial b" & _
          " WHERE 1=0" & _
          " ORDER BY a.nEntryNox"
  
   Set p_oSerialOld = New adodb.Recordset
   p_oSerialOld.Open lsSQL, p_oAppDrivr, adOpenKeyset, adLockOptimistic, adCmdText
   Set p_oSerialOld.ActiveConnection = Nothing
   
endProc:
   Exit Sub
errProc:
    ShowError lsProcName & "( " & " )"
End Sub

Private Sub p_oBaseClas_LoadOthers()
   Dim bHasSerial As Boolean
   With p_oBaseClas
      p_sAddressx = .Master("xAddressx")
      For pnCtr = 0 To .ItemCount - 1
         .Detail(pnCtr, "nQtyOnHnd") = .Detail(pnCtr, "xQtyOnHnd")
                     
         'Load Serials if the stock is serialized
         If bHasSerial = False And .Detail(pnCtr, "cHsSerial") = xeYes Then
            bHasSerial = True
         End If
      Next
      If bHasSerial Then
         Call loadSerialOnCache
      End If
   End With
End Sub

