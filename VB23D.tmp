Private Sub p_oBaseClas_SaveOthers(Cancel As Boolean)
   Dim lors As Recordset
   Dim lsEntryNox As String
   Dim lsProcName As String
   Dim lsSQL As String
   
   lsProcName = "p_oBaseClas_SaveOthers"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   Cancel = True

   With p_oBaseClas
      With p_oCPTrans
         .Branch = p_sBranchCd
         Set .AppDriver = p_oAppDrivr
         If .InitTransaction() = False Then GoTo endProc
      End With
      lsStockIDx = ""
      For pnCtr = 0 To .ItemCount - 1
         If .Detail(pnCtr, "sStockIDx") = "" Then Exit For
         
         p_oCPTrans.Detail(pnCtr, "sStockIDx") = .Detail(pnCtr, "sStockIDx")
         p_oCPTrans.Detail(pnCtr, "nQuantity") = .Detail(pnCtr, "nQuantity")
         p_oCPTrans.Detail(pnCtr, "nQtyOnHnd") = .Detail(pnCtr, "nQtyOnHnd")
         p_oCPTrans.Detail(pnCtr, "nLedgerNo") = IIf(.Detail(pnCtr, "nLedgerNo") = "", 0, .Detail(pnCtr, "nLedgerNo"))
         p_oCPTrans.Detail(pnCtr, "cHsSerial") = .Detail(pnCtr, "cHsSerial")
             
         'Transfer serials to the clsCPTransfer object if serialized
         lsEntryNox = lsEntryNox & ", " & pnCtr
         If .Detail(pnCtr, "cHsSerial") = xeYes Then
            If saveSerial( _
               pnCtr, _
               .Detail(pnCtr, "nQuantity"), _
               .DetailOriginal(pnCtr, "nEntryNox")) Then GoTo endProc
            End If
         End If
      Next
      
      If .EditMode <> xeModeAddNew Then
         lsSQL = "SELECT *" & _
                " FROM CP_Transfer_Serial" & _
                " WHERE sTransNox = " & strParm(.Master("sTransNox")) & _
                  " AND nEntryNox NOT IN (" & Mid(lsEntryNox, 2) & ")"
         Set lors = p_oAppDrivr.Connection.Execute(lsSQL, , adCmdText)
         
         If lors.RecordCount > 0 Then
            lsSQL = "DELETE" & _
                   " FROM CP_Transfer_Serial" & _
                   " WHERE sTransNox = " & strParm(.Master("sTransNox")) & _
                     " AND nEntryNox NOT IN (" & Mid(lsEntryNox, 2) & ")"
           p_oAppDrivr.Execute lsSQL, "CP_Transfer_Serial", Left(.Master("sTransNox"), 2), .Master("sDestinat")
         End If
      End If
               
      ' if a CP_Inventory is not successfully updated, cancel saving.
      If p_oCPTrans.Delivery(.Master("sTransNox"), _
            .Master("dTransact"), _
            .EditMode) = False Then
         GoTo endProc
      End If
      
      ' check if reorder level of some parts has reached
      If p_oCPTrans.ReOrderParts Then
         MsgBox "Some Parts Included in the Transaction has Reached it Reorder Level Limit!!!" & _
                  vbCrLf & vbCrLf & "Please Inform the CP_Inventory Manager on this Alert!!!", _
                  vbInformation, "Notice"
      End If
      
   End With
   
   Cancel = False

endProc:
   Exit Sub
errProc:
   ShowError lsProcName & "( " & Cancel & " )"
End Sub

Private Sub p_oBaseClas_WillCancel(Cancel As Boolean)
   Dim lsProcName As String
   
   lsProcName = "p_oBaseClas_WillCancel"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   
   Cancel = Not delSPTransaction

endProc:
   Exit Sub
errProc:
    ShowError lsProcName & "( " & Cancel & " )"
End Sub

Private Sub p_oBaseClas_WillDelete(Cancel As Boolean)
   Dim lsProcName As String
   Dim lsSQL As String
   Dim lnCtr As Long
   
   lsProcName = "p_oBaseClas_WillDelete"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   
   Cancel = Not delSPTransaction

endProc:
   Exit Sub
errProc:
   ShowError lsProcName & "( " & Cancel & " )"
End Sub

Private Sub p_oBaseClas_WillSave(Cancel As Boolean)
   Dim lsProcName As String

   lsProcName = "p_oBaseClas_WillSave"
   'Debug.Print pxeMODULENAME & "." & lsProcName
   On Error GoTo errProc
   Cancel = True

   pbModified = True
   With p_oBaseClas
      If .isMasterModify = False And .isDetailModify = False Then
         pbModified = False
         Cancel = False
         GoTo endProc
      End If
   
      'MS-2007.12.18
      If .EditMode = xeModeAddNew Then
         .Master("sTransNox") = GetNextCode(.MasterTable, "sTransNox", True, _
                                    p_oAppDrivr.Connection, True, p_sBranchCd)
      End If
   
      For pnCtr = 0 To .ItemCount - 1
         If .Detail(pnCtr, "sStockIDx") = "" Then Exit For
         
         .Detail(pnCtr, "sTransNox") = .Master("sTransNox")
         .Detail(pnCtr, "nEntryNox") = pnCtr + 1
            
      Next
   End With
   
   Cancel = False

endProc:
   Exit Sub
errProc:
   ShowError lsProcName & "( " & Cancel & " )"
End Sub

Private Sub ShowError(ByVal lsProcName As String)
    With p_oAppDrivr
        .xLogError Err.Number, Err.Description, pxeMODULENAME, lsProcName, Erl
    End With
    With Err
        .Raise .Number, .Source, .Description
    End With
End Sub
